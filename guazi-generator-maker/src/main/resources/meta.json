{
  "name": "acm-template-pro-generator",
  "description": "Acm 示例模板生成器",
  "basePackage": "com.whn",
  "version": "1.0",
  "author": "whn",
  "createTime": "2024-1-28",
  "fileConfig": {
    "inputRootPath": "D:/JAVA/workspace/guazi-generator/guazi-generator-demo-projects/acm-template-pro",
    "outputRootPath": "generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/whn/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/whn/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": " 是否生成循环",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": " 作者注释",
        "defaultValue": "whn",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  }
}